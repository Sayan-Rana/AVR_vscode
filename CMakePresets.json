{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 19,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "avr",
      "displayName": "AVR-GCC Build",
      "generator": "Ninja",
      "description": "AVR build with avr-gcc and ninja",
      "binaryDir": "${sourceDir}/build",
      "cacheVariables": {
        "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/toolchain-avr.cmake"
      }
    }
  ]
}
